  <style type="text/css">
	.progressBar
	{
		position: relative;  
		width: 100%;  
		height:20px;  
		background-color: #eee;  
	}  
	.timeBar
	{
		position: absolute;  
		top: 0;  
		left: 0;  
		width: 0;  
		height: 100%;  
		background-color: #88CC88;  
	}
	.bufferedBar
	{
		position: absolute;  
		top: 0;  
		left: 0;  
		width: 0;  
		height: 100%;  
		background-color: #BBCCBB;
	}
